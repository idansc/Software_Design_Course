package il.ac.technion.cs.sd.lib.clientserver;


public class ServerTaskEndedMessage extends  DataPacket{
	
	/* Just a magic number to represent a message indicating the ServerTask was
	 * completed.
	 */
	public static final byte[] SERVER_TASK_ENDED_MESSAGE 
		= "QL2OTk6AapDq6QV7XOjJ".getBytes();
		
}