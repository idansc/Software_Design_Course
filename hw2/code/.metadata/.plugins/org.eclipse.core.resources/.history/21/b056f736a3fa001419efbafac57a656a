package il.ac.technion.cs.sd.lib.clientserver;


@FunctionalInterface
public interface ServerTask {
	
	/* @param server The server processing this task (it'll be the same server in all
	 * subsecuent calls the 'run' */
	 * @param data The data that the client sent to the server. */
	void run(Server server, String clientAddress, byte[] data);
}
